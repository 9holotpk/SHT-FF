{
    "applications": {
        "gecko": {
            "id": "iShortener@9holotpk",
            "strict_min_version": "51.0"
        }
    },
    "author": "9holotpk",
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "browser_action": {
        "default_icon": {
            "128": "icons/link_128.png"
        },
        "default_title": "Shorten URL & QR Code",
        "default_popup": "popup.html"
    },
    "description": "Fast Shorten URL for Web",
    "developer": {
        "url": "http://twitter.com/9holotpk",
        "name": "Sontaya T. Wichakot"
    },
    "icons": {
        "16": "icons/link_16.png",
        "64": "icons/link_64.png",
        "128": "icons/link_128.png"
    },
    "manifest_version": 2,
    "name": "iShortener",
    "permissions": [
        "tabs",
        "storage",
        "activeTab",
        "<all_urls>",
        "clipboardWrite",
        "clipboardRead"
    ],
    "short_name": "iShortener",
    "version": "1.4"
}