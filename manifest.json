{
    "applications": {
        "gecko": {
            "id": "iShortener@9holotpk",
            "strict_min_version": "51.0"
        }
    },
    "author": "9holotpk",
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "page_action": {
        "default_icon": {
            "19": "icons/link_19.png",
            "38": "icons/link_38.png"
        },
        "default_title": "Shorten URL & QR Code",
        "default_popup": "popup.html",
        "show_matches": [
            "<all_urls>"
        ]
    },
    "description": "Fast Shorten URL for Web",
    "developer": {
        "url": "http://twitter.com/ishortener",
        "name": "Sontaya T. Wichakot"
    },
    "icons": {
        "16": "icons/link_16.png",
        "64": "icons/link_64.png",
        "128": "icons/link_128.png"
    },
    "manifest_version": 2,
    "name": "iShortener",
    "permissions": [
        "storage",
        "activeTab",
        "clipboardWrite"
    ],
    "short_name": "iShortener",
    "version": "1.6.3"
}